generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model OrderNotification {
  id             String   @id @default(cuid())
  notificationId String   @unique
  action         String
  apiVersion     String
  applicationId  String
  dateCreated    DateTime
  liveMode       Boolean
  type           String
  userId         Int
  orderId        String // Vem de data.id
}