generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model OrderNotification {
  id            String   @id 
  action        String
  apiVersion    String
  applicationId String
  dateCreated   DateTime
  liveMode      Boolean
  type          String
  userId        Int
  dataId        String

  @@map("order_notifications")
}
