{
  "name": "apimercadopago",
  "version": "1.0.0",
  "description": "API para capturar dados do mercado pago e exibi-los",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node --watch --watch-path=src src/app.js",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:push": "npx prisma db push",
    "db:deploy": "npx prisma migrate deploy",
    "test:mock": "node test/send-mock-webhook.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apolock33/apimercadopago.git"
  },
  "keywords": [
    "api",
    "mercadopago"
  ],
  "author": "apolock33",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Apolock33/apimercadopago/issues"
  },
  "homepage": "https://github.com/Apolock33/apimercadopago#readme",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
